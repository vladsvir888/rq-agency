    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.3",
    "esbuild": "^0.19.11",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "js-beautify": "^1.14.11",
    "node-sass-glob-importer": "^5.3.3",
    "postcss": "^8.4.33",
    "postcss-sort-media-queries": "^5.2.0",
    "prettier": "^3.1.1",
    "sass": "^1.69.7",
    "stylelint": "^16.1.0",
    "stylelint-config-standard-scss": "^13.0.0"
